extends layout.pug
include utils.pug
block content

    +validationErrorSummary(errors)
    form(action='/users/login' method='post')
        input(type='hidden' name='_csrf' value=csrfToken)
        +field('Email', 'email', 'Demo123@demo.com')
        br
        +field('Password', 'password', 'Demo123@demo.com', 'password')
        br
        button(type='submit') Submit
